#, c-format
msgid "Uploading translations to %s failed."
msgstr "Загрузка переводов в %s не удалась."

msgid "Syncing error"
msgstr "Ошибка синхронизации"

msgid "Add"
msgstr "Добавить"

msgid "Unknown Crowdin error."
msgstr "Неизвестная ошибка Crowdin."

msgid "Not authorized, please sign in again."
msgstr "Не авторизованы, пожалуйста войдите снова."

msgid "Downloading translations is disabled in this project."
msgstr "Загрузка переводов отключена в этом проекте."

msgid "Sign In"
msgstr "Войти"

msgid "Sign in"
msgstr "Войти"

msgid "Sign Out"
msgstr "Выйти"

msgid "Sign out"
msgstr "Выйти"

